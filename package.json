    "@types/node": "^22.5.1",
    "@vitejs/plugin-vue": "^5.1.3",
    "allof-merge": "^0.6.6",
    "autoprefixer": "^10.4.20",
    "eslint": "8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "scule": "^1.3.0",
    "shiki": "^1.16.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitepress": "1.3.4",
    "vitest": "^2.0.5",
    "vue": "^3.4.38",
    "vue-i18n": "^9.13.1",
    "vue-json-pretty": "^2.4.0",
    "vue-tsc": "^2.1.4",
    "xml-formatter": "^3.6.3"
